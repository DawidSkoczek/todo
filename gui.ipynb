    "import functions\n",
    "\n",
    "import PySimpleGUI as sg\n",
    "\n",
    "label = sg.Text('Type in a to-do')\n",
    "input_box = sg.InputText(tooltip='Enter todo')\n",
    "add_button = sg.Button('Add')\n",
    "\n",
    "winodw = sg.Window('My To-Do App', layout=[[label], [input_box, add_button]])\n",
    "\n",
    "winodw.read()\n",
    "winodw.close()"
